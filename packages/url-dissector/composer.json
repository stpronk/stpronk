{
    "name": "stpronk/url-dissector",
    "description": "Parse URLs into component parts and store them in a normalized database structure with FilamentPHP v4 integration.",
    "keywords": [
        "laravel",
        "filament",
        "url",
        "parser",
        "analytics"
    ],
    "homepage": "https://github.com/stpronk/url-dissector",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Pronk",
            "email": "steve@pronk.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "jeremykendall/php-domain-parser": "^6.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Stpronk\\UrlDissector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stpronk\\UrlDissector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stpronk\\UrlDissector\\UrlDissectorServiceProvider"
            ],
            "aliases": {
                "UrlDissector": "Stpronk\\UrlDissector\\Facades\\UrlDissector"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
