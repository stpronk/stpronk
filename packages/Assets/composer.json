{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "stpronk/filamentphp-assets",
  "description": "Assets package for managing asset resources (name, category, price in cents) with Filament resource and migrations.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Stpronk\\Assets\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stpronk\\Assets\\AssetsServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.3",
    "laravel/framework": "^12.0",
    "filament/filament": "^4.0",
    "stpronk/filamentphp-essentials": "@dev"
  }
}
