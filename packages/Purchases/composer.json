{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "stpronk/filamentphp-purchases",
  "description": "A package for tracking items that need to be purchased.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Stpronk\\Purchases\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stpronk\\Purchases\\PurchasesServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.3",
    "laravel/framework": "^12.0",
    "filament/filament": "^4.0",
    "stpronk/filamentphp-essentials": "@dev",
    "stpronk/url-dissector": "@dev"
  }
}
